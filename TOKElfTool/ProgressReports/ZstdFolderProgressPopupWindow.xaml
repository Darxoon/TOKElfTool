<Window x:Class="TOKElfTool.ProgressReports.ZstdFolderProgressPopupWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TOKElfTool.ProgressReports"
        mc:Ignorable="d"
        Title="ZstdFolderProgressPopupWindow" Height="107.475" Width="231.388" Loaded="ZstdFolderProgressPopupWindow_OnLoaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <Label Margin="5">Saving...</Label>
        <ProgressBar x:Name="progressBar"  Grid.Row="1" Minimum="0" Maximum="1" Value="0" Height="18" Margin="8,0,8,8"></ProgressBar>
    </Grid>
</Window>
