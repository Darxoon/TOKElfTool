<UserControl x:Class="TOKElfTool.ObjectEditControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TOKElfTool"
             xmlns:st="http://schemas.spicytaco.io/"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Expander x:Name="Expander">
        <st:AutoGrid x:Name="Grid"  Columns="*,*">
            <Button Margin="5" Click="RemoveButton_OnClick">Remove</Button>
            <Button Margin="5" Click="DuplicateButton_OnClick">Duplicate</Button>
        </st:AutoGrid>
    </Expander>
</UserControl>
